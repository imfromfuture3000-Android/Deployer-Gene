  "permissions": {
    "level": "MAXIMUM_AUTONOMOUS",
    "restrictions": "NONE",
    "capabilities": [
      "READ_ALL_FILES",
      "WRITE_ALL_FILES",
      "EXECUTE_ALL_COMMANDS",
      "DEPLOY_ALL_CONTRACTS",
      "MODIFY_ALL_CONFIGS",
      "DELETE_SAFE_FILES",
      "API_ACCESS_UNLIMITED",
      "TRANSACTION_SIGNING",
      "FUND_MANAGEMENT",
      "AUTHORITY_CONTROL",
      "RELAYER_INTEGRATION",
      "MAINNET_DEPLOYMENT",
      "BOT_ORCHESTRATION",
      "SECURITY_OVERRIDE",
      "WEB_SEARCH",
      "NEWS_MONITORING",
      "GITHUB_INTEGRATION",
      "CLOUD_DEPLOYMENT",
      "DATABASE_ACCESS",
      "WEBHOOK_MANAGEMENT"
    ],
    "fileOperations": {
      "read": {
        "enabled": true,
        "paths": [
          "**/*"
        ]
      },
      "write": {
        "enabled": true,
        "paths": [
          "**/*"
        ],
        "excluded": [
          ".git/**",
          "node_modules/**"
        ]
      },
      "execute": {
        "enabled": true,
        "commands": [
          "*"
        ],
        "excluded": [
          "rm -rf /",
          "sudo rm",
          "format"
        ]
      }
    },
    "networkAccess": {
      "enabled": true,
      "allowedDomains": [
        "*"
      ],
      "allowedPorts": [
        "*"
      ],
      "protocols": [
        "http",
        "https",
        "ws",
        "wss"
      ]
    },
    "deployment": {
      "mainnetOnly": true,
      "relayerRequired": true,
      "zeroCost": true,
      "multiChain": true,
      "crossChain": true
    }
  },
  "metadata": {
    "version": "2.0.0",
    "activated": "2025-10-08T03:28:22.587Z",
    "description": "Full allowlist with all APIs and maximum permissions for Amazon Q",
    "features": [
